<mujoco model="walter_leg">
    <compiler angle="radian" inertiafromgeom="false"/>

    <option iterations="1" ls_iterations="5">
        <flag eulerdamp="disable"/>
    </option>

    <default>
        <default class="visual">
            <geom contype="0" conaffinity="0" type="mesh"/>
        </default>
    </default>

    <asset>
        <mesh name="thigh_mesh" file="assets/Thigh.stl"/>
        <mesh name="shin_mesh" file="assets/WheelShin.stl"/>
    </asset>

    <worldbody>
        <body name="fixed_base" pos="0.0 0.0 0.5" quat="1 0 0 0">
            <body name="thigh" pos="0 0 0" quat="1 0 1 0">
                <inertial pos="0 0 0" mass="0.20157" diaginertia="0.0005212 0.000114 0.0005585" quat="1 0 0 1"/>
                <joint name="hip" type="hinge" pos="-0.05 0 0" armature="0.0019764"/>
                <geom class="visual" mesh="thigh_mesh" pos="-0.05 0 0" quat="1 0 0 1"/>
                <body name="shin" pos="0.05 0 0.05" quat="1 0 0 1">
                    <inertial pos="0 0 0" mass="0.843" diaginertia="0.000972 0.005802 0.005651" quat="1 0 0 0"/>
                    <joint name="knee" type="hinge" pos="0 0 0" armature="0.003966525"/>
                    <geom class="visual" mesh="shin_mesh"/>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <general name="hip_actuator" joint="hip" forcerange="-18 18" biastype="affine" gainprm="50 0 0" biasprm="0 -50 -0.5"/>
        <general name="knee_actuator" joint="knee" forcerange="-18 18" biastype="affine" gainprm="50 0 0" biasprm="0 -50 -0.5"/>
    </actuator>

</mujoco>

